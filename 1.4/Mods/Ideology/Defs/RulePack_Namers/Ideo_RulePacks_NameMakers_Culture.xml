<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ==========[ESCP_Falmer_NamerCultureLeaderTitle]========== -->
	<RulePackDef>
		<defName>ESCP_Falmer_NamerCultureLeaderTitle</defName>
		<include>
			<li>LeaderTitleMaker_GlobalStructure</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>memeLeaderNoun(priority=1)->[cultureLeaderNoun]</li>
				<!-- ignore all meme nouns, tribals always use a culture noun -->
				<li>cultureLeaderTitle->[cultureLeaderAdjective] [cultureLeaderNoun]</li>
				<li>cultureLeaderAdjective->great</li>
				<li>cultureLeaderAdjective->high</li>
				<li>cultureLeaderAdjective->first</li>
				<li>cultureLeaderAdjective->grand</li>
				<li>cultureLeaderAdjective->knight</li>
				<li>cultureLeaderNoun->chancellor</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>
	<!-- ==========[ESCP_Falmer_NamerCultureDeityName]========== -->
	<RulePackDef>
		<defName>ESCP_Falmer_NamerCultureDeityName</defName>
		<include>
			<li>NamerDeityGlobal</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>name->[FalmerGods]</li>
			</rulesStrings>
			<rulesFiles>
				<li>FalmerGods->ESCP_FalmerNames/FalmerGods</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>
</Defs>
